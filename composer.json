{
  "name": "old-town/zf2-metadata-generator",
  "description": "Generator .phpstorm.meta.php",
  "type": "library",
  "keywords": [
    "zf2",
    "phpstorm",
    "metadata",
    "zf2 module"
  ],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Malofeykin Andrey",
      "email": "and-rey2@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "zendframework/zend-modulemanager": "~2.4.0",
    "zendframework/zend-mvc": "~2.4.0",
    "zendframework/zend-servicemanager": "~2.4.0"
  },
  "require-dev": {
    "ext-curl": "*",
    "fabpot/php-cs-fixer": "1.9.1",
    "phpunit/phpunit": "4.7.7",
    "phpunit/phpcov": "2.0.1",
    "zendframework/zend-loader": "~2.4.0",
    "zendframework/zend-test": "~2.4.0",
    "zendframework/zend-serializer": "~2.4.0",
    "zendframework/zend-log": "~2.4.0",
    "zendframework/zend-i18n": "~2.4.0",
    "zendframework/zend-text": "~2.4.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "OldTown\\Zf2MetadataGenerator\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OldTown\\Zf2MetadataGenerator\\PhpUnit\\Test\\": "test/phpunit/tests",
      "OldTown\\Zf2MetadataGenerator\\PhpUnit\\TestData\\": "test/phpunit/_files"
    },
    "classmap": [
      "./test/phpunit/Bootstrap.php"
    ]
  }
}
